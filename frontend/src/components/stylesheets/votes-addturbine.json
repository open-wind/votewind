[
    {
      "id": "votes-halo",
      "type": "symbol",
      "source": "votes",
      "layout": {
          "icon-image": "tick-outline", 
          "icon-size": 0.13,
          "icon-anchor": "bottom",
          "icon-allow-overlap": true,
          "icon-offset": [0, 190]
        },
      "paint": {
        "icon-color": "#2563EB",
        "icon-opacity": ["case", ["boolean", ["feature-state", "selected"], false], 1, 0]
      }
    },
    {
      "id": "votes-unconfirmed",
      "type": "symbol",
      "source": "votes",
      "filter": [">=", ["get", "votes_unconfirmed"], 1],
      "layout": {
          "icon-image": "tick-desaturated-dropshadow", 
          "icon-size": 0.1,
          "icon-anchor": "bottom",
          "icon-allow-overlap": true
      }
    },
    {
      "id": "votes-confirmed",
      "type": "symbol",
      "source": "votes",
      "filter": [">=", ["get", "votes_confirmed"], 1],
      "layout": {
          "icon-image": "tick-dropshadow", 
          "icon-size": 0.1,
          "icon-anchor": "bottom",
          "icon-allow-overlap": true
      }
    }
]